{
  "fileTypes": [],
  "injectionSelector": "L:markup.fenced_code.block.markdown",
  "patterns": [
      {
          "include": "#haskell-code-block"
      }
  ],
  "repository": {
      "haskell-code-block": {
          "begin": "\\b(aya|aya-prover)\\b(\\s+[^`~]*)?$",
          "end": "(^|\\G)(?=\\s*[`~]{3,}\\s*$)",
          "patterns": [
              {
                  "begin": "(^|\\G)(\\s*)(.*)",
                  "while": "(^|\\G)(?!\\s*([`~]{3,})\\s*$)",
                  "contentName": "meta.embedded.block.aya",
                  "patterns": [
                      {
                          "include": "source.ard"
                      }
                  ]
              }
          ]
      }
  },
  "scopeName": "markdown.aya.codeblock"
}